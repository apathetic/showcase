{
  "name": "Components",
  "version": "0.1.0",
  "description": "A showcase of HUGE components",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://stash.hugeinc.com/projects/CMPTS/repos/showcase/browse"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-remove-console": "^6.0.14",
    "babel-polyfill": "^6.0.14",
    "babel-preset-es2015": "^6.0.15",
    "ejs": "^2.3.4",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "harp": "^0.20.0",
    "webpack": "^1.12.2",
    "noodlejs": "^0.3.2"
  },
  "scripts": {
    "start": "harp server",
    "build": "node generator.js",
    "dev": "webpack --progress --colors --watch",
    "prod": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "compile": "node_modules/.bin/harp compile",
    "git-commit": "git add www && git commit -m 'gh-pages update'",
    "git-push": "git push origin `git subtree split --prefix www master`:gh-pages --force",
    "xxxxs": "git subtree push --prefix www origin gh-pages",
    "deploy": "npm run compile && npm run git-commit && npm run git-push"
  },
